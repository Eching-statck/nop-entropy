<?xml version="1.0" encoding="UTF-8" ?>
<workflow x:schema="/nop/schema/wf/wf.xdef" xmlns:x="/nop/schema/xdsl.xdef">

    <diagram/>

    <steps>
        <step displayName="开始步骤" name="wf-start">
            <assignment>
                <actors>
                    <actor actorId="actor1" actorType="user" actorModelId="actor1"/>
                </actors>
            </assignment>
            <transition>
                <to-step stepName="subFlow"/>
            </transition>
        </step>
        <flow name="subFlow">
            <start wfName="test/subflow"/>
            <transition>
                <to-end/>
            </transition>
        </flow>
    </steps>

    <start startStepName="wf-start"/>
</workflow>